<% if flash[:success].present? %>
  $('#edu-group-tab-<%= @group_object.id %>').html('<%= j render partial: "users", locals: {users: @group_object.users, group: @group_object} %>');
  $.growl.notice({message: '<%= flash[:success] %>'});
  $('.modal[group=<%= @group_object.id %>]').modal('hide');
<% else %>
  $.growl.error({message: '<%= flash[:danger] %>'});
  $('.modal[group=<%= @group_id %>]').modal('hide');
<% end %>
