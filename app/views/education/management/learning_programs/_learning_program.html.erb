<% learning_programs.each do |learning_program| %>
  <tr>
    <td><%= learning_program.name %></td>
    <td><%= learning_program.description %></td>
    <td>
      <button class="btn btn-primary" data-toggle="modal"
        data-target="#modal_learning_program_<%= learning_program.id %>">
        <%= t ".edit" %>
      </button>
    </td>
  </tr>
  <%= render "edit", learning_program: learning_program %>
<% end %>
